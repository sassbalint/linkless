{
  "manifest_version": 2,
  "name": "Linkless",
  "description": "Removes distracting links so you can focus better.",
  "homepage_url": "https://github.com/sassbalint/linkless",
  "version": "1.0",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./linkless.js"]
    }
  ]
}
